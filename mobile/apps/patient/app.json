{
  "expo": {
    "name": "Ocura360-Patient",
    "slug": "ocura360-patient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://via.placeholder.com/1024",
    "userInterfaceStyle": "automatic",
    "scheme": "ocura360patient",
    "splash": {
      "image": "https://via.placeholder.com/1284x2778",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ocura360.patient",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to find nearby clinics."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://via.placeholder.com/1024",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ocura360.patient",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "YOUR_EAS_PROJECT_ID"
      }
    }
  }
}
